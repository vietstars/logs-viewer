{
    "name": "vietstars/logs-viewer",
    "description": "A Logs viewer",
    "license": "MIT",
    "keywords": [
        "logs",
        "logs-viewer",
        "logging",
        "laravel"
    ],
    "type": "laravel-package",
    "authors": [
        {
            "name": "vietstars",
            "email": "vietstar.nt@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7|^9.5.10",
        "orchestra/testbench": "3.7.*|^7.0"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "Vietstars\\LogsViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vietstars\\LogsViewer\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vietstars\\LogsViewer\\LogsViewerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
